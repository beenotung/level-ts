{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "sourceRoot": "./src"
  },
  "linterOptions": {},
  "jsRules": true,
  "rules": {
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [
      true,
      256
    ],
    "ordered-imports": false,
    "no-implicit-dependencies": false,
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-console": [
      true,
      "log",
      "debug",
      "info"
    ]
  },
  "rulesDirectory": []
}